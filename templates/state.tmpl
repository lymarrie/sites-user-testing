---
{
    "streamId": "pages-state-stream",
    "urlFormat": "{{.id}}"
}
---

{{define "state.tmpl" -}}
<html>
    <body>
        <h1> Welcome to - {{.name}}</h1>

        <p>
        <h3> Other cities in this state:</h3>
        {{ range $key, $value := .c_children}}
          <li><strong>{{ $key }}</strong>: {{ $value }}</li>
        {{end}}
        </p>

    </body>
</html>
{{end}}
