---
{
    "streamId": "pages-state-stream",
    "urlFormat": "{{.c_slug}}"
}
---

{{define "state.tmpl" -}}
<html>
    <body>
        <h1> Welcome to - {{.name}}</h1>

        <p> Cities in this state: 
            {{ range .c_children }}
                {{ range $key, $value := . }}
                    <li><strong>{{ $key }}</strong>: {{ $value }}</li>
                {{ end }}
            {{ end }}
        </p>

    </body>

</html>
{{end}}
